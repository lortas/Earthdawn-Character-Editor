				<xs:documentation>Is calculated by the attribute lpcostgrowth.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="deed" type="xs:string" default=""/>
		<xs:attribute name="effect" type="xs:string" default=""/>
